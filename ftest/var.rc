#!/usr/bin/env rc

func test (num uint; desc string; status bool) {
  if status {
    println (`ok `, num, ` - `, desc);
  } else {
    println (`not ok `, num, ` - `, desc);
  }
}

type Test component { };

init (this *Test) Init () {
  println (`1..1`);
  var x const uint = 3;
  test (1, `var x const = ...`, x == 3);
}

instance system Test Init ();
